#define CARDS_IN_DECK 52
//structure defining a card
typedef struct card {
    int suit; // 56,57,58,59 wrt, heart, spades,clubs,diamonds
    int value;// 1,2....10,11,12,13, ace being lowest card at value 1
} Card;

//prototypes for functions being used
void shuffleDeck(Card* deck);
void printDeck(Card* deck, int numCards, int topDeck);
void comparison(Card userStack[50],Card cpuStack[50]);
